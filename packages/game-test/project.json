{
  "name": "@nicholasosto/game-test",
  "sourceType": "game-test",
  "tags": ["scope:roblox", "type:app"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "command": "rbxtsc --type game",
        "cwd": "packages/game-test"
      },
      "cache": true,
      "inputs": ["default"],
      "outputs": ["{projectRoot}/out"]
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rbxtsc -w --type game",
        "cwd": "packages/game-test"
      }
    },
    "rojo-serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rojo serve",
        "cwd": "packages/game-test"
      }
    },
    "rojo-build": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "rojo build default.project.json --output game-test.rbxl",
        "cwd": "packages/game-test"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf out include",
        "cwd": "packages/game-test"
      }
    }
  }
}
