{
  "name": "@nicholasosto/node-tools",
  "sourceType": "node-tools",
  "tags": ["scope:node", "type:lib"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.build.json",
        "cwd": "packages/node-tools"
      },
      "cache": true,
      "inputs": ["default"],
      "outputs": ["{projectRoot}/dist"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "packages/node-tools"
      },
      "cache": true,
      "inputs": ["default"],
      "outputs": []
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "packages/node-tools"
      }
    }
  }
}
